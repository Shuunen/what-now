{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/what-now/issues",
  "description": "Minimalistic task coach",
  "devDependencies": {
    "@camwiegert/typical": "0.1.1",
    "cypress": "4.3.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-purgecss": "2.1.3",
    "parcel-plugin-static-files-copy": "2.3.1",
    "posthtml-include": "1.4.3",
    "repo-check": "1.5.1",
    "rimraf": "3.0.2",
    "serialize-to-js": "3.1.1",
    "serve": "11.3.0",
    "shuutils": "4.0.0",
    "tinygesture": "1.1.4"
  },
  "files": [
    "src",
    "static"
  ],
  "homepage": "https://github.com/Shuunen/what-now",
  "keywords": [
    "minimalist",
    "task",
    "coach"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/what-now",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/what-now.git"
  },
  "scripts": {
    "build": "parcel build src/index.html --public-url . --no-source-maps --detailed-report",
    "check": "repo-check",
    "ci": "npm run test",
    "dev": "run-p watch test:open",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.html .",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist/*",
    "preversion": "npm run ci",
    "serve": "serve dist --listen 8000 --single",
    "start": "npm run watch",
    "test": "npm-run-all check lint build --parallel serve test:headless --race",
    "test:headless": "cypress run",
    "test:open": "cypress open",
    "update": "npx npm-check-updates -u",
    "watch": "parcel src/index.html --port 8000"
  },
  "version": "0.0.2"
}
